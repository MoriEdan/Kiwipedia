{
	"db_name": "ZeroWikipedia",
	"db_file": "ZeroWikipedia.db",
	"version": 3,
	"maps": {
		"(^|.+/)data/users/.+/content.json": {
			"to_json_table": ["cert_user_id"],
			"file_name": "data.json"
		},
		"^data/users/.+/data.json": {
			"to_table": [
				"hubs"
			]
		}
	},
	"tables": {
		"json": {
			"cols": [
				["json_id", "INTEGER PRIMARY KEY AUTOINCREMENT"],
				["site", "TEXT"],
				["directory", "TEXT"],
				["file_name", "TEXT"],
				["cert_user_id", "TEXT"]
			],
			"indexes": ["CREATE UNIQUE INDEX path ON json(directory, site, file_name)"],
			"schema_changed": 4
		},
		"hubs": {
			"cols": [
				["language", "TEXT"],
				["subgroup", "TEXT"],
				["address", "TEXT"],
				["slug", "TEXT"],
				["json_id", "INTEGER PRIMARY KEY AUTOINCREMENT"]
			],
			"schema_changed": 2
		}
	}
}